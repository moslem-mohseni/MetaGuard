# MetaGuard Environment Configuration
# Author: Moslem Mohseni
# Copy this file to .env and modify as needed

# Model settings
METAGUARD_MODEL_PATH=
METAGUARD_MODEL_TYPE=random_forest

# Detection settings
METAGUARD_RISK_THRESHOLD=0.5

# Logging settings
METAGUARD_LOG_LEVEL=INFO
METAGUARD_LOG_FILE=
METAGUARD_LOG_JSON=false

# Performance settings
METAGUARD_BATCH_SIZE=100
METAGUARD_N_JOBS=-1

# Feature engineering
METAGUARD_USE_FEATURE_ENGINEERING=true
